{
  "version": "0.2.0",
  "configurations": [
      {
          "command": "npm start",
          "name": "Run npm start",
          "request": "launch",
          "type": "node-terminal"
      },
    {
      "name": "Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/src",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      }
    }
  ]
}